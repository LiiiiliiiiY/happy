page {
    background-color: #F9F9F9;
}
.price{
    width: 100%;
    background-color: #fff;
    margin-top: 15rpx;
    padding-top: 20rpx;
	padding-bottom: 20rpx;
    box-sizing: border-box;
}
.title{
	display: block;
    font-size:29rpx;
    font-weight:bold;
    color:rgba(0,0,0,1);
    padding-left: 28rpx;
	padding: 30rpx 0 10rpx 28rpx;
}
.input_{
    margin-top: 10rpx;
    width: 100%;
    border-bottom: 1px solid #999;
    margin-bottom: 30rpx;
    display: flex;
    align-items: center;
}
.input_ text{
    font-size:74rpx;
    padding-left: 40rpx;
    color:rgba(0,0,0,1);
}
.input_ input{
    width: 520rpx;
}
.input_ .all{
    font-size:28rpx;
    color:rgba(0,177,236,1);
}
.des{
    display: block;
    font-size:25rpx;
    margin-bottom: 20rpx;
    color:rgba(153,153,153,1);
    padding: 0 110rpx 0 28rpx;
}
.method{
    width:100%;
    height:81rpx;
    line-height:81rpx;
    font-size:27rpx;
    font-weight:bold;
    color:rgba(153,153,153,1);
    background:rgba(255,255,255,1);
    margin-top: 48rpx;
    padding-left: 28rpx;
    box-sizing: border-box;
}
button{
    display: block;
    width:611rpx!important;
    margin: 30rpx auto;
    height:79rpx;
    background:rgba(0,177,236,1);
    border-radius:10rpx!important;
    font-size:35rpx!important;
	font-weight: 400!important;
    color:rgba(255,255,255,1);
    font-weight: 400!important;
    line-height: 1.2!important;
}