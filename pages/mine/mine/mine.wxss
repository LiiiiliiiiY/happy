page {
	background-color: #F5F6F7;
	height: 100%;
}
.con {
	/* position: relative; */
	width: 100%;
	padding: 30rpx 20rpx 25rpx;
	color: #333333;
	box-sizing: border-box;
}
.top{
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.top .left{
	padding: 0!important;
	margin: 0!important;
}
.top .left,.top .right{
	display: flex;
	align-items: center;
}
.top .right button{
	padding: 0!important;
	margin: 0!important;
	width: auto!important;
	line-height: 0!important;
}
.top .left image{
	width:90rpx;
	margin-right: 14rpx;
	border-radius: 50%;
}
.top .left text{
	font-size:34rpx;
	font-weight:bold;
	color:rgba(0,0,0,1);
}
.top .right image{
	width:52rpx;
	margin-left: 10rpx;
}
.yue{
	margin-top: 40rpx;
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: space-around;
	width:706rpx;
	background:linear-gradient(90deg,rgba(255,104,105,1) 10%,rgba(248,0,0,1) 100%);
	border-radius:10rpx;
}
.yue > view{
	position: relative;
	display: flex;
	align-items: center;
	flex-direction: column;
	padding: 35rpx 0px;
	color: #fff;
	min-width: 230rpx;
}
.yue > view .title:nth-child(1){
	font-size: 27rpx;
	display: flex;
	align-items: center;
}
.yue > view .title:nth-child(1) image{
	width: 34rpx;
	margin-left: 5rpx;
}
.yue > view .text:nth-child(2){
	margin-top: 20rpx;
	font-size: 64rpx;
}
.yue > view .btn{
	margin-top: 20rpx;
	width:130rpx;
	height:45rpx;
	line-height:45rpx;
	text-align: center;
	background:rgba(255,255,255,1);
	border-radius:10rpx;
	font-size:29rpx;
	color:rgba(255,1,1,1);
}
.yue > view:nth-child(1):after{
	content: '';
	position: absolute;
	right: -60rpx;
	top: 60rpx;
	width:2rpx;
	height:157rpx;
	background-color: #fff;
}
.yaoqing{
	position: relative;
	width: 100%;
	display: flex;
	align-items: center;
	padding-bottom: 14rpx;
	padding-left: 15rpx;
	padding-top: 30rpx;
	border-bottom: 2rpx solid #ccc;
	background-color: #fff;
	box-sizing: border-box;
}
.yaoqing image{
	width: 58rpx;
	margin-right: 16rpx;
}
.yaoqing text{
	font-size:36rpx;
	font-weight:bold;
	color:rgba(0,0,0,1);
}
.yaoqing view{
	position: fixed;
	top: 630rpx;
	right: 28rpx;
	z-index: 999;
	width:95rpx;
	height:95rpx;
	padding: 10rpx;
	box-sizing: border-box;
	text-align: center;
	line-height:35rpx;
	font-size:26rpx;
	color:rgba(255,255,255,1);
	border:6rpx solid rgba(255,255,255,1);
	background:linear-gradient(90deg,rgba(255,104,105,1) 10%,rgba(248,0,0,1) 100%);
	box-shadow:0px 0px 11rpx 0px rgba(0, 0, 0, 0.06);
	border-radius:50%;
}
.person{
	display: flex;
	align-items: center;
	justify-content: space-around;
	width: 100%;
	padding: 30rpx 0 45rpx;
	background-color: #fff;
	border-bottom-left-radius: 10rpx;
	border-bottom-right-radius: 10rpx;
}
.person .view{
	display: flex;
	flex-direction: column;
	align-items: center;
}
.person .view text:nth-child(1){
	font-size:55rpx;
	font-weight:bold;
	color:rgba(0,0,0,1);
}
.person .view view:nth-child(2){
	font-size:29rpx;
	color:rgba(0,0,0,1);
	display: flex;
	align-items: center;
}
.person .view view:nth-child(2) image{
	width: 34rpx;
	margin-left: 5rpx;
}
.bt{
	height: 25rpx;
	width: 100%;
	background: #fff;
}
.order{
	position: relative;
	display: flex;
	align-items: center;
	justify-content: space-between;
	width: 100%;
	margin-top: 18rpx;
	padding-top: 16rpx;
	padding-left: 15rpx;
	padding-right: 10rpx;
	padding-bottom: 42rpx;
	box-sizing: border-box;
	background-color: #fff;
	border-top-left-radius: 10rpx;
	border-top-right-radius: 10rpx;
}
.order::after{
	content: '';
	position: absolute;
	left: 0;
	bottom: 26rpx;
	width: 100%;
	height: 1px;
	background: #f9f9f9;
}
.order > view{
	display: flex;
	align-items: center;
}
.order > view image{
	width: 46rpx;
	margin-right: 22rpx;
}
.order > view text{
	font-size:36rpx;
	font-weight:bold;
	color:rgba(0,0,0,1);
}
.order > .text{
	display: flex;
	align-items: center;
	font-size:24rpx;
	color:rgba(104,104,104,1);
}
.order > .text image{
	width: 34rpx;
	margin-left: 5rpx;
}
.order-list{
	width: 100%;
	border-bottom: 2rpx solid #ccc;
	padding: 20rpx 35rpx 28rpx;
	box-sizing: border-box;
	background-color: #fff;
	overflow: hidden;
}
.order-list .order-title{
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: space-between;
	box-sizing: border-box;
	padding-left: 15rpx;
	margin-bottom: 10rpx;
}
.order-list .order-title text:nth-child(1){
	position: relative;
	font-size:25rpx;
	font-weight:bold;
	color:rgba(63,63,63,1);
}
.order-list .order-title text:nth-child(1):after{
	content: '';
	position: absolute;
	top: -1rpx;
	left: -25rpx;
	width:11rpx;
	height:39rpx;
	background:rgba(255,0,0,1);
	border-radius:6rpx;
}
.order-list .order-title text:nth-child(2){
	font-size:21rpx;
	color:rgba(255,0,0,1);
}
.order-content{
	display: flex;
	align-items: center;
	justify-content: space-between;
	width: 100%;
	box-sizing: border-box;
	margin-top: 20rpx;
}
.order-content > image{
	width: 128rpx;
}
.order-content > navigator{
	width: 470rpx;
}
.order-content .first{
	display: flex;
	align-items: center;
	justify-content: space-between;
	width: 100%;
	margin-bottom: 18rpx;
}
.order-content .first text:nth-child(1){
	width: 360rpx;
	height: 40rpx;
	font-size:31rpx;
	font-weight:bold;
	color:rgba(63,63,63,1);
	overflow: hidden; 
	text-overflow: ellipsis;
	white-space: nowrap; 
}
.order-content .first text:nth-child(2){
	font-size:20rpx;
	color:rgba(146,146,146,1);
}
.order-content .second{
	display: flex;
	align-items: center;
	justify-content: space-between;
	width: 100%;
}
.order-content .second > text:nth-child(1){
	font-size:20rpx;
	color:rgba(146,146,146,1);
}
.order-content .second  view:nth-child(2){
	font-size:27rpx;
	color:rgba(0,0,0,1);
}
.order-content .second  view:nth-child(2) .price{
	color:rgba(255,26,54,1);
}
.subtotal{
	margin-top: 20rpx;
	margin-bottom: 20rpx;
	font-size:31rpx;
	font-weight:bold;
	color:rgba(63,63,63,1);
}
.subtotal text{
	 color: #FF0000;
}
.btn-box{
	float: right;
	display: flex;
	margin-top: 15rpx;
}
.btn-box text,.btn-box navigator{
	width: 148rpx;
	font-size:24rpx;
	width:148rpx;
	height:46rpx;
	border-radius:23rpx;
	text-align: center;
	line-height: 46rpx;
	margin-left: 10rpx;
}
.btn-box .white{
	color:rgba(63,63,63,1);
	border:2rpx solid rgba(63,63,63,1);
	background-color: #fff;
}
.btn-box .red{
	color:rgba(255,255,255,1);
	background:rgba(255,0,0,1);
	border:2rpx solid rgba(255,0,0,1);
}
.nav{
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: space-around;
	padding: 26rpx 0rpx 16rpx;
	box-sizing: border-box;
	background-color: #fff;
	border-bottom-left-radius:10px;
	border-bottom-right-radius:10px;
}
.nav .view{
	display: flex;
	flex-direction: column;
	align-items: center;
}
.nav .view:nth-child(1) image{
	width: 55rpx;
}
.nav .view:nth-child(2) image{
	width: 55rpx;
}
.nav .view:nth-child(3) image{
	width: 55rpx;
}
.nav .view:nth-child(4) image{
	width: 55rpx;
}
.nav .view:nth-child(5) image{
	width: 55rpx;
}
.nav .view:nth-child(6) image{
	width: 55rpx;
}
.nav .view text{
	margin-top: 4rpx;
	font-size:24rpx;
	color:rgba(104,104,104,1);
}
.help{
	width: 706rpx;
	margin: 12rpx auto 66rpx;
	background-color: #fff;
	border-radius:10rpx;
}
.help > .view{
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 0 18rpx 0 10rpx!important;
	margin: 0!important;
	background-color: #fff!important;
	width: 100%;
	font-weight: normal!important;
	box-sizing: border-box;
	height: 80rpx;
	border-bottom: 1px solid #f9f9f9;
}
.help > .view:nth-child(4){
	border-bottom: 0px solid #f9f9f9;
	border-bottom-left-radius: 10rpx;
	border-bottom-right-radius: 10rpx;
}
.help > .view text{
	color:rgba(104,104,104,1);
}
.help > .view text:nth-child(1){
	font-size:24rpx;
}
.help > .view image:nth-child(2){
	width:34rpx;
}
.recommend{
	margin-bottom: 40rpx;
}
.recommend-title{
	display: block;
	width: 100%;
	text-align: center;
	position: relative;
	font-size:39rpx;
	font-weight:bold;
	margin-bottom: 74rpx;
	color:rgba(255,26,54,1);
}
.recommend-title::before{
	content: '';
	position: absolute;
	top: 25rpx;
	left: 0;
	height: 3rpx;
	width: 250rpx;
	background-color: #FF1A36;
}
.recommend-title::after{
	content: '';
	position: absolute;
	top: 25rpx;
	right: 0;
	height: 3rpx;
	width: 250rpx;
	background-color: #FF1A36;
}
.recommend-list{
	width: 700rpx;
	margin: 0rpx auto;
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding-bottom: 35rpx;
	padding-top: 35rpx;
	padding-left: 13rpx;
	padding-right: 20rpx;
	border-bottom: 1px solid #f9f9f9;
	box-sizing: border-box;
	background-color: #fff;
}
.recommend-list:first-child{
	border-top-left-radius: 10rpx;
	border-top-right-radius: 10rpx;
}
.recommend-list:last-child{
	border-bottom-left-radius: 10rpx;
	border-bottom-right-radius: 10rpx;
}
.recommend-list > navigator > image{
	width: 214rpx;
	margin-right: 20rpx;
}
.recommend-list > view{
	width: 450rpx;
}
.recommend-list > view text:nth-child(1){
	display: block;
	width: 420rpx;
	height: 40rpx;
	font-size:36rpx;
	font-weight:bold;
	color:rgba(63,63,63,1);
	overflow: hidden; 
	text-overflow: ellipsis;
	white-space: nowrap; 
	line-height: 39rpx;
}
.recommend-list > view view.white{
	width:240rpx;
	height:40rpx;
	line-height: 40rpx;
	text-align: center;
	font-size:29rpx;
	color:rgba(255,255,255,1);
	background-color: #FF1A36;
	border-radius:10px;
	margin: 20rpx 0 10rpx;
}
.recommend-list > view text:nth-child(3){
	font-size:26rpx;
color:rgba(153,153,153,1);
}
.recommend-list > view .price{
	display: flex;
	align-items: center;
	justify-content: space-between;
	margin-top: 18rpx;
}
.recommend-list > view .price text:nth-child(1){
	width: auto;
	font-size:29rpx;
	font-weight:bold;
	color: #FF1A36;
}
.recommend-list > view .price > view{
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.recommend-list > view .price > view .view:nth-child(1){
	width:150rpx;
	text-align: center;
	height:45rpx;
	line-height:45rpx;
	border:1rpx solid rgba(236,19,20,1);
	border-radius:10rpx;
	font-size:32rpx;
	font-weight:bold;
	color:rgba(255,26,54,1);
	background-color: #fff;
	margin: 0;
}
.recommend-list > view .price > view .view:nth-child(2){
	width:150rpx;
	text-align: center;
	height:45rpx;
	margin-left: 10rpx;
	background:rgba(236,19,20,1);
	border:1px solid rgba(236,19,20,1);
	border-radius:10rpx;
	font-size:32rpx;
	font-weight:bold;
	color:#fff;
}



.quxiaodialog{
	position: fixed;
	top: 300rpx;
	left: 10%;
	z-index: 100;
	background: #fff;
	padding: 97rpx 100rpx 66rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	border-radius:35rpx;
}
.quxiaodialog image{
	width: 343rpx;
	margin-bottom: 58rpx;
}
.quxiaodialog>text:nth-child(2){
	font-size:37rpx;
	color:rgba(0,0,0,1);
	margin-bottom: 25rpx;
}
.quxiaodialog>text:nth-child(3){
	font-size:28rpx;
	color:rgba(145,145,145,1);
	margin-bottom: 80rpx;
}
.quxiaodialog > .view{
	width:396rpx;
	height:85rpx;
	line-height: 85rpx;
	background:rgba(255,0,0,1);
	border-radius:20rpx;
	font-size:37rpx;
	color:rgba(255,255,255,1);
	text-align: center;
}
.shade{
	position: fixed;
	top: 0rpx;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 50;
	background: #000;
	opacity: 0.7;
}
.inforShade{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%!important;
	height: 100%!important;
	padding: 0;
	background-color: #FFFFFF;
	z-index: 100;
	opacity: 0;
}




.ad {
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 100;
	background-color: rgba(0, 0, 0, 0.75);
}
.ad-container {
	position: absolute;
	left: 50%;
	top: 50%;
	transform: translate(-50%, -50%);
}
.get-infor {
	width: 750rpx!important;
	padding: 0;
	margin: 0;
	background-color: rgba(0,0,0,0);
}
.ad-container .ad-img {
	width: 750rpx;
}
.ad-container .close {
	position: absolute;
	left: 50%;
	bottom: -90rpx;
	width: 110rpx;
	transform: translateX(-50%);
}